{ 
    "_id" : "variantCalls", 
    "displayNameSingular" : "Variant Call", 
    "displayNamePlural" : "Variant Calls", 
    "description" : "This data type describes the requirements for a variant call.", 
    "validationSchema" : {
        "#dollar#schema" : "http://json-schema.org/draft-07/schema#", 
        "#dollar#async" : true, 
        "title" : "EVA Sequence variation checklist", 
        "author" : "USI", 
        "type" : "object", 
        "description" : "EVA sequence variation analysis.", 
        "version" : "1.0.0", 
        "required" : [
            "alias", 
            "title", 
            "description", 
            "sampleRefs", 
            "studyRefs", 
            "attributes", 
            "files"
        ], 
        "properties" : {
            "alias" : {
                "type" : "string", 
                "minLength" : 1.0
            }, 
            "title" : {
                "type" : "string", 
                "minLength" : 1.0
            }, 
            "description" : {
                "type" : "string", 
                "minLength" : 1.0
            }, 
            "studyRefs" : {
                "type" : "array", 
                "minItems" : 1.0, 
                "maxItems" : 1.0, 
                "items" : {
                    "#dollar#ref" : "#/definitions/submittableRef"
                }
            }, 
            "sampleRefs" : {
                "type" : "array", 
                "minItems" : 1.0, 
                "items" : {
                    "#dollar#ref" : "#/definitions/submittableRef"
                }
            }, 
            "attributes" : {
                "type" : "object", 
                "required" : [
                    "experiment type"
                ], 
                "oneOf" : [
                    {
                        "required" : [
                            "assembly name"
                        ]
                    }, 
                    {
                        "required" : [
                            "assembly accession"
                        ]
                    }
                ], 
                "patternProperties" : {
                    "^#dot#*$" : {
                        "minItems" : 1.0, 
                        "type" : "array", 
                        "items" : {
                            "required" : [
                                "value"
                            ], 
                            "properties" : {
                                "terms" : {
                                    "#dollar#ref" : "#/definitions/terms"
                                }, 
                                "value" : {
                                    "type" : "string", 
                                    "minLength" : 1.0
                                }, 
                                "units" : {
                                    "type" : "string", 
                                    "minLength" : 1.0
                                }
                            }
                        }
                    }
                }, 
                "properties" : {
                    "assembly name" : {
                        "minItems" : 1.0, 
                        "maxItems" : 1.0, 
                        "description" : "Name of the reference sequence"
                    }, 
                    "assembly accession" : {
                        "minItems" : 1.0, 
                        "maxItems" : 1.0, 
                        "description" : "Accession of the reference sequence"
                    }, 
                    "experiment type" : {
                        "minItems" : 1.0, 
                        "maxItems" : 1.0, 
                        "items" : {
                            "properties" : {
                                "value" : {
                                    "type" : "string", 
                                    "enum" : [
                                        "Whole genome sequencing", 
                                        "Exome sequencing", 
                                        "Genotyping by array", 
                                        "Curation", 
                                        "Genotyping by sequencing"
                                    ]
                                }
                            }
                        }
                    }, 
                    "program" : {
                        "description" : "Programs used when performing this analysis"
                    }, 
                    "pipeline description" : {
                        "description" : "Textual description of the pipeline used to call the variants.", 
                        "maxItems" : 1.0
                    }, 
                    "platform" : {
                        "description" : "Experimental platform(s) used when generating data used in this analysis", 
                        "items" : {
                            "properties" : {
                                "value" : {
                                    "type" : "string", 
                                    "enum" : [
                                        "Illumina HiSeq 2000", 
                                        "Illumina HiSeq 2500", 
                                        "Illumina HiSeq 1000", 
                                        "Illumina HiScanSQ", 
                                        "Illumina MiSeq", 
                                        "Illumina Genome Analyzer", 
                                        "Illumina Genome Analyzer II", 
                                        "Illumina Genome Analyzer IIx", 
                                        "AB SOLiD System 2.0", 
                                        "AB SOLiD System 3.0", 
                                        "AB SOLiD 3 Plus System", 
                                        "AB SOLiD 4 System", 
                                        "AB SOLiD 4hq System", 
                                        "AB SOLiD PI System", 
                                        "AB 5500 Genetic Analyzer", 
                                        "AB 5500xl Genetic Analyzer", 
                                        "AB SOLiD System", 
                                        "454 GS FLX", 
                                        "454 GS", 
                                        "454 GS 20", 
                                        "454 GS FLX+", 
                                        "454 GS FLX Titanium", 
                                        "454 GS Junior", 
                                        "Complete Genomics", 
                                        "unspecified"
                                    ]
                                }
                            }
                        }
                    }, 
                    "imputation" : {
                        "maxItems" : 1.0, 
                        "description" : "Was imputation used in this analysis?", 
                        "items" : {
                            "properties" : {
                                "value" : {
                                    "type" : "string", 
                                    "enum" : [
                                        "true", 
                                        "false"
                                    ]
                                }
                            }
                        }
                    }, 
                    "phasing" : {
                        "maxItems" : 1.0, 
                        "description" : "Was phasing used in this analysis?", 
                        "items" : {
                            "properties" : {
                                "value" : {
                                    "type" : "string", 
                                    "enum" : [
                                        "true", 
                                        "false"
                                    ]
                                }
                            }
                        }
                    }, 
                    "date" : {
                        "description" : "Date the analysis was performed", 
                        "maxItems" : 1.0, 
                        "items" : {
                            "properties" : {
                                "value" : {
                                    "type" : "string", 
                                    "format" : "date"
                                }
                            }
                        }
                    }
                }
            }, 
            "files" : {
                "type" : "array", 
                "minItems" : 1.0, 
                "items" : {
                    "type" : "object", 
                    "properties" : {
                        "name" : {
                            "type" : "string", 
                            "minLength" : 1.0
                        }, 
                        "type" : {
                            "type" : "string", 
                            "enum" : [
                                "vcf"
                            ]
                        }
                    }, 
                    "required" : [
                        "name", 
                        "type"
                    ]
                }
            }
        }, 
        "definitions" : {
            "terms" : {
                "items" : {
                    "required" : [
                        "url"
                    ], 
                    "type" : "object", 
                    "properties" : {
                        "url" : {
                            "type" : "string", 
                            "format" : "uri", 
                            "isValidTerm" : true
                        }
                    }
                }, 
                "type" : "array"
            }, 
            "submittableRef" : {
                "type" : "object", 
                "properties" : {
                    "alias" : {
                        "type" : "string", 
                        "minLength" : 1.0
                    }, 
                    "accession" : {
                        "type" : "string", 
                        "minLength" : 1.0
                    }, 
                    "team" : {
                        "type" : "string", 
                        "minLength" : 1.0
                    }
                }, 
                "anyOf" : [
                    {
                        "required" : [
                            "alias"
                        ]
                    }, 
                    {
                        "required" : [
                            "accession"
                        ]
                    }
                ]
            }
        }
    }, 
    "refRequirements" : [
        {
            "refClassName" : "SampleRef", 
            "dataTypeIdForReferencedDocument" : "samples", 
            "additionalRequiredValidationAuthors" : [

            ]
        }
    ], 
    "requiredValidationAuthors" : [
        "Ena", 
        "Core", 
        "JsonSchema", 
        "FileReference"
    ], 
    "optionalValidationAuthors" : [

    ], 
    "submittableClassName" : "uk.ac.ebi.subs.repository.model.Analysis", 
    "archive" : "Ena"
}